{
  "name": "lyricova-blog",
  "version": "1.0.0",
  "description": "A lyrics-focused blog for Project Lyricova",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint && npm run build-next",
    "serve": "ts-node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -n node,tscr -c green,blue npm:watch-node npm:watch-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-next": "next build",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && yarn run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "yarn run watch serve-debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueset/project-lyricova.git"
  },
  "author": "Eana Hufwe",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/blueset/project-lyricova/issues"
  },
  "homepage": "https://github.com/blueset/project-lyricova#readme",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@mui/x-data-grid": "^5.17.24",
    "connect-session-sequelize": "^7.1.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "graphql": "^15.7.2",
    "lodash": "^4.17.21",
    "next": "^13.1.6",
    "notistack": "^2.0.8",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}
