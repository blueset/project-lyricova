{
  "name": "lyricova-blog",
  "version": "2.0.0",
  "description": "A lyrics-focused blog for Project Lyricova",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint && npm run build-next",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -n node,tscr -c green,blue npm:watch-node npm:watch-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-next": "next build",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && yarn run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "yarn run watch serve-debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueset/project-lyricova.git"
  },
  "author": "Eana Hufwe",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/blueset/project-lyricova/issues"
  },
  "homepage": "https://github.com/blueset/project-lyricova#readme",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@mui/x-data-grid": "^5.17.24",
    "@next/font": "^13.2.3",
    "@resvg/resvg-js": "^2.4.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@vercel/og": "^0.4.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^1.2.1",
    "colorjs.io": "^0.4.3",
    "connect-session-sequelize": "^7.1.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "framer-motion": "^10.2.3",
    "graphql": "^15.7.2",
    "lodash": "^4.17.21",
    "matter-attractors": "^0.1.6",
    "matter-js": "^0.19.0",
    "next": "^13.1.6",
    "next-superjson": "^0.0.4",
    "notistack": "^2.0.8",
    "p5": "^1.6.0",
    "rss": "^1.2.2",
    "superjson": "^1.12.2",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.2.4",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/matter-js": "^0.18.2",
    "@types/node": "^18.14.0",
    "@types/p5": "^1.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/rss": "^0.0.30",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.4",
    "sass": "^1.58.3",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}
