  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint && npm run build-next",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -n node,tscr -c green,blue npm:watch-node npm:watch-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-next": "next build",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug-b",
    "watch-node-debug": "nodemon --inspect dist/server.js",
    "watch-debug-b": "npm run watch serve-debug",
    "watch-debug": "concurrently -n node,tscr -c green,blue npm:watch-node-debug npm:watch-ts"
  },
