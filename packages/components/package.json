    "axios": "^1.12.0",
    "base64url": "^3.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
